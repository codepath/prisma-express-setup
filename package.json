{
  "name": "prisma-express-setup",
  "version": "0.0.1",
  "author": "gabriel@cabrejas.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/codepath/prisma-express-setup.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "node --watch index.js",
    "start": "node index.js",
    "build": "npm install && npm run db:init",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "node prisma/seed.js",
    "db:init": "npm run db:reset && npm run db:migrate && npm run db:seed",
    "psql": "psql shelterdb -U app_user"
  },
  "dependencies": {
    "@prisma/client": "^6.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "prisma": "^6.8.1"
  }
}
