{
  "name": "prisma-express-setup",
  "version": "0.0.1",
  "author": "Gabriel Cabrejas",
  "repository": {
    "type": "git",
    "url": "https://github.com/codepath/prisma-express-setup.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "node --env-file=.env --watch index.js",
    "start": "node index.js",
    "build": "npm run db:setup",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init --skip-generate",
    "db:reset": "prisma migrate reset --force --skip-seed --skip-generate",
    "db:setup": "npm run db:reset && npm run db:migrate && npm run db:generate && npm run db:seed",
    "db:seed": "node prisma/seed.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.8.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "prisma": "^6.8.1"
  }
}
