{
  "name": "prisma-express-setup",
  "version": "0.0.1",
  "author": "gabriel@cabrejas.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/codepath/prisma-express-setup.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "node --env-file=.env --watch index.js",
    "start": "node index.js",
    "build": "npm install && npm run db:init",
    "db:migrate": "prisma migrate dev --name init --skip-generate",
    "db:reset": "prisma migrate reset --force --skip-seed --skip-generate",
    "db:init": "npm run db:reset && npm run db:migrate && prisma generate && npm run db:seed",
    "db:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "prisma": "^6.8.1"
  }
}
